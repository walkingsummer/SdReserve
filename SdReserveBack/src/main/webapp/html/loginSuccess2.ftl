<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <title>Sd reserve back manage login success</title>
    <script src="reserveSys/resources/scripts/jquery-1.7.1.min.js"></script>
    <script>
        $(document).ready(
            function () {
                $("#btn1").click(function () {
                    $.ajax({
                        type:"post",
                        dataType:"json",
                        contentType:"application/json",
                        url:"/doLogout.do",
//                        data:"",
                        success:function (data) {
                            alert(JSON.stringify(data));
                            window.location = "/html/login.html";
                        }
                    });
                });
                $("#btn2").click(function () {
//                    alert(GetQueryString("error"));
//                    alert(GetQueryString("msg"));

                    alert(${error});
                    alert(${msg});
                });
            }
        );

        function GetQueryString(name)
        {
            var reg = new RegExp("(^|&)"+ name +"=([^&]*)(&|$)");
            var r = window.location.search.substr(1).match(reg);
            if(r!=null)return  unescape(r[2]); return null;
        }

    </script>


</head>
<body>
    <h1>您已经登录成功</h1>
    <button id="btn1">退出登录</button>

    <button id="btn2">[#if error!=null](${error})[/#if]</button>

</body>
</html>